<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.washington.briluu.quizdroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MathActivity" />
        <activity android:name=".PhysicsActivity" />
        <activity android:name=".MarvelSuperHeroesActivity" />
        <activity android:name=".QuestionActivity" />
        <activity android:name=".AnswerActivity" />
        <activity android:name=".LastAnswerActivity"></activity>
    </application>

</manifest>